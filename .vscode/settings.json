{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "codemetrics.luaconfiguration.ReturnStatement": 0,
  "codemetrics.nodeconfiguration.ReturnStatement": 0,
  "codemetrics.basics.CodeLensHiddenUnder": 0,
  "codemetrics.basics.Exclude": [
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "codemetrics.basics.ComplexityLevelExtremeDescription": "💀💀💀We need to improve it urgently.",
  "codemetrics.basics.ComplexityLevelHighDescription": "🥵Let's improve！",
  "codemetrics.basics.ComplexityLevelLowDescription": "👍Good job!",
  "codemetrics.basics.ComplexityLevelLow": -1,
  "codemetrics.basics.ComplexityLevelNormalDescription": "🙁Just a little more！",
  "codemetrics.basics.ComplexityTemplate": "複雑度: {0} {1}",
  "codemetrics.nodeconfiguration.ArrowFunction": 0,
  "codemetrics.nodeconfiguration.JsxElement": 0,
  "codemetrics.nodeconfiguration.JsxSelfClosingElement": 0,
  "tailwindCSS.experimental.classRegex": [
    [
      "classNames\\(([^)]*)\\)",
      "[\"']([^'\"]*)[\"']"
    ]
  ],
}